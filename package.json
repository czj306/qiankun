{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "install:dev": "yarn install",
    "install:main": "cd main && yarn install",
    "install:secondary": "cd secondary && yarn install",
    "build:main": "cd main && yarn build && mv ./dist ../dist/main",
    "build:secondary": "cd secondary && yarn build && mv ./dist ../dist/secondary",
    "install-all": "npm-run-all --parallel install:*",
    "build": "npm-run-all --parallel build:*"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "homepage": "https://czj306.github.io/qiankun"
}
